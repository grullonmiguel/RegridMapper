<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:views="clr-namespace:RegridMapper.Views"
                    xmlns:viewModels="clr-namespace:RegridMapper.ViewModels">

    <DataTemplate DataType="{x:Type viewModels:ParcelViewModel}">
        <views:ParcelView />
    </DataTemplate>
    
    <DataTemplate DataType="{x:Type viewModels:RealAuctionViewModel}">
        <views:RealAuctionView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewModels:MapViewModel}">
        <views:MapView />
    </DataTemplate>

    <!-- Dialogs -->
    <DataTemplate DataType="{x:Type viewModels:StateDialogViewModel}" >
        <views:StateDialogView/>
    </DataTemplate>

</ResourceDictionary>